.wrap{
    padding: 50px 20px;
}
.wrap .btn-wrap{
    width: 50%;
    float: left;
    text-align: center;
}
.updateForm .formHeader{
    width: 100%;
    height: 60px;
    line-height: 60px;
    border-bottom: 1px solid #a1a1a1;
}
.updateForm .formHeader .formTitle{
    font-family: '微软雅黑';
    font-size: 22px;
    display: inline-block;
    margin-left: 20px;
    float: left;
    color:#333333;
}
.updateForm .formHeader .updating,.saving,.canceling,.goback{
    font-family: '微软雅黑';
    font-size: 18px;
    display: inline-block;
    float: right;
    margin-right: 15px;
    color:#ffffff;
    margin-top: 10px;
}
.container-fluid .formDisplay{
    display: block;
}
.tag{
    font-size: 18px;
    color:#efefef;
    display: inline-block;
    height: 50px;
    line-height: 50px;
    float: right;
}
.saving,.canceling{
    display: none;
}
.form .form-group{
    width: 50%;
    display: inline-block;
    float: left;
}
.form span{
    font-size: 14px;
    font-family: "Microsoft YaHei";
    display: block;
    height: 34px;
    line-height: 34px;
    color:#333333;
}
.qty_unit,.price_unit{
    margin-bottom: 0px;
}
.freight_type{
    border:1px solid #a1a1a1;
}
.ul{
    width: 100%;
}
.ul .Item{
    list-style: none;
    width: 100%;
    min-height: 36px;
    clear: both;
}