.tag{
    font-size: 18px;
    color:#efefef;
    display: inline-block;
    height: 50px;
    line-height: 50px;
    float: right;
}
.updateForm .formHeader{
    width: 100%;
    height: 60px;
    line-height: 60px;
    border-bottom: 1px solid #a1a1a1;
}
.updateForm .formHeader .formTitle{
    font-family: '微软雅黑';
    font-size: 22px;
    display: inline-block;
    margin-left: 20px;
    float: left;
    color:#333333;
}
.updateForm .formHeader .updating,.saving,.canceling,.goback{
    font-family: '微软雅黑';
    font-size: 18px;
    display: inline-block;
    float: right;
    margin-right: 15px;
    color:#ffffff;
    margin-top: 10px;
}
.formSubmit{
    width: 70%;
    margin-left: 15%;
    margin-right: 15%;
}
.formSubmit .form .form-group{
    width: 100%;
}
.freight_type{
    width: 100%;
    border:1px solid #a1a1a1;
    font-size: 14px;
    color:#444444;
    height: 35px;
    line-height: 35px;
    font-family: '微软雅黑';
}