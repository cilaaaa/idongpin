.font-family {
  font-family: "Microsoft YaHei";
}
body {
  background: #f5f5f5;
}
.header {
  width: 100%;
  height: 1.18rem;
  line-height: 1.18rem;
  text-align: center;
  background: #e02341;
  position: relative;
}
.header .back {
  position: absolute;
  left: 0.43rem;
  display: inline-block;
  width: 0.32rem;
  height: 0.56rem;
  margin-top: 0.31rem;
}
.header .back img {
  width: 0.32rem;
  height: 0.56rem;
  float: left;
}
.header .page_ttl {
  color: #ffffff;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .header .page_ttl {
  font-size: 18px;
}
[data-dpr="2"] .header .page_ttl {
  font-size: 36px;
}
[data-dpr="3"] .header .page_ttl {
  font-size: 54px;
}
.content {
  width: 100%;
}
.content .container {
  width: 10rem;
  margin: 0 auto;
}
.content .container .goods_picture {
  width: 10rem;
}
.content .container .goods_picture img {
  width: 10rem;
}
.content .container .goods_info {
  width: 10rem;
  margin-top: 0.02rem;
  background: #ffffff;
}
.content .container .goods_info .goods_name {
  width: 10rem;
  box-sizing: border-box;
  padding: 0.26rem 0.43rem;
  line-height: 0.62rem;
  color: #4d4d4d;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .goods_info .goods_name {
  font-size: 15px;
}
[data-dpr="2"] .content .container .goods_info .goods_name {
  font-size: 30px;
}
[data-dpr="3"] .content .container .goods_info .goods_name {
  font-size: 45px;
}
.content .container .goods_info .goods_price {
  width: 10rem;
  height: 0.46rem;
  line-height: 0.46rem;
  box-sizing: border-box;
  padding: 0 0.43rem;
  color: #e02341;
  font-weight: bold;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .goods_info .goods_price {
  font-size: 17px;
}
[data-dpr="2"] .content .container .goods_info .goods_price {
  font-size: 34px;
}
[data-dpr="3"] .content .container .goods_info .goods_price {
  font-size: 51px;
}
.content .container .goods_info .goods_origin {
  width: 10rem;
  height: 0.65rem;
  line-height: 0.62rem;
  box-sizing: border-box;
  padding: 0 0.43rem;
  color: #999999;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .goods_info .goods_origin {
  font-size: 11px;
}
[data-dpr="2"] .content .container .goods_info .goods_origin {
  font-size: 22px;
}
[data-dpr="3"] .content .container .goods_info .goods_origin {
  font-size: 33px;
}
.content .container .purcher_count {
  width: 10rem;
  height: 1.34rem;
  line-height: 1.34rem;
  background: #ffffff;
  box-sizing: border-box;
  padding: 0 0.43rem;
  border-top: 0.02rem solid #e8e8e8;
  border-bottom: 0.02rem solid #e8e8e8;
}
.content .container .purcher_count .count_container {
  float: right;
}
.content .container .purcher_count .count_container .count_txt {
  float: left;
  color: #1a1a1a;
  font-family: "Microsoft YaHei";
  margin-right: 0.2rem;
}
[data-dpr="1"] .content .container .purcher_count .count_container .count_txt {
  font-size: 15px;
}
[data-dpr="2"] .content .container .purcher_count .count_container .count_txt {
  font-size: 30px;
}
[data-dpr="3"] .content .container .purcher_count .count_container .count_txt {
  font-size: 45px;
}
.content .container .purcher_count .count_container .count_machine {
  margin-top: 0.34rem;
  width: 2.34rem;
  height: 0.66rem;
  float: right;
  border: 0.02rem solid #999999;
  border-radius: 0.07rem;
}
.content .container .purcher_count .count_container .count_machine .subtract {
  width: 0.65rem;
  height: 0.66rem;
  line-height: 0.66rem;
  text-align: center;
  float: left;
  color: #4d4d4d;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .purcher_count .count_container .count_machine .subtract {
  font-size: 15px;
}
[data-dpr="2"] .content .container .purcher_count .count_container .count_machine .subtract {
  font-size: 30px;
}
[data-dpr="3"] .content .container .purcher_count .count_container .count_machine .subtract {
  font-size: 45px;
}
.content .container .purcher_count .count_container .count_machine .count {
  width: 1rem;
  height: 0.66rem;
  line-height: 0.66rem;
  text-align: center;
  border-right: 0.02rem solid #999999;
  border-left: 0.02rem solid #999999;
  float: left;
  color: #e02341;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .purcher_count .count_container .count_machine .count {
  font-size: 15px;
}
[data-dpr="2"] .content .container .purcher_count .count_container .count_machine .count {
  font-size: 30px;
}
[data-dpr="3"] .content .container .purcher_count .count_container .count_machine .count {
  font-size: 45px;
}
.content .container .purcher_count .count_container .count_machine .add {
  width: 0.65rem;
  height: 0.66rem;
  line-height: 0.66rem;
  text-align: center;
  float: left;
  color: #4d4d4d;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .purcher_count .count_container .count_machine .add {
  font-size: 15px;
}
[data-dpr="2"] .content .container .purcher_count .count_container .count_machine .add {
  font-size: 30px;
}
[data-dpr="3"] .content .container .purcher_count .count_container .count_machine .add {
  font-size: 45px;
}
.content .container .purcher_count .count_container .count_machine .clear {
  clear: both;
}
.content .container .purcher_count .count_container .clear {
  clear: both;
}
.content .container .present_info {
  width: 10rem;
  height: 1.64rem;
  border-top: 0.02rem solid #e8e8e8;
  border-bottom: 0.02rem solid #e8e8e8;
  margin-top: 0.21rem;
  box-sizing: border-box;
  padding: 0.21rem 0.43rem;
  background: #ffffff;
}
.content .container .present_info .send_txt {
  width: 0.96rem;
  height: 1.64rem;
  line-height: 0.6rem;
  float: left;
  color: #999999;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .present_info .send_txt {
  font-size: 11px;
}
[data-dpr="2"] .content .container .present_info .send_txt {
  font-size: 22px;
}
[data-dpr="3"] .content .container .present_info .send_txt {
  font-size: 33px;
}
.content .container .present_info .send_detail {
  width: 7.92rem;
  height: 1.64rem;
  float: left;
}
.content .container .present_info .send_detail .send_address {
  width: 7.92rem;
  height: 0.6rem;
  line-height: 0.6rem;
}
.content .container .present_info .send_detail .send_address .address_icon {
  width: 0.48rem;
  height: 0.6rem;
  float: left;
}
.content .container .present_info .send_detail .send_address .address_icon img {
  width: 0.48rem;
  height: 0.6rem;
}
.content .container .present_info .send_detail .send_address .address {
  float: left;
  margin-left: 0.12rem;
  color: #1a1a1a;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .present_info .send_detail .send_address .address {
  font-size: 15px;
}
[data-dpr="2"] .content .container .present_info .send_detail .send_address .address {
  font-size: 30px;
}
[data-dpr="3"] .content .container .present_info .send_detail .send_address .address {
  font-size: 45px;
}
.content .container .present_info .send_detail .send_address .stock {
  float: left;
  margin-left: 0.53rem;
  color: #e02341;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .present_info .send_detail .send_address .stock {
  font-size: 15px;
}
[data-dpr="2"] .content .container .present_info .send_detail .send_address .stock {
  font-size: 30px;
}
[data-dpr="3"] .content .container .present_info .send_detail .send_address .stock {
  font-size: 45px;
}
.content .container .present_info .send_detail .arrive_date {
  width: 7.92rem;
  height: 0.6rem;
  line-height: 0.6rem;
  box-sizing: border-box;
  padding-left: 0.6rem;
  color: #999999;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .present_info .send_detail .arrive_date {
  font-size: 12px;
}
[data-dpr="2"] .content .container .present_info .send_detail .arrive_date {
  font-size: 24px;
}
[data-dpr="3"] .content .container .present_info .send_detail .arrive_date {
  font-size: 36px;
}
.content .container .present_info .send_more {
  width: 0.26rem;
  height: 1.64rem;
  line-height: 1.64rem;
  float: left;
}
.content .container .present_info .clear {
  clear: both;
}
.content .container .goods_info {
  width: 10rem;
  background: #ffffff;
}
.content .container .goods_info .ttl {
  display: block;
  width: 10rem;
  height: 0.84rem;
  line-height: 0.84rem;
  border-right: 0.02rem solid #999999;
  border-left: 0.02rem solid #999999;
  box-sizing: border-box;
  padding: 0 0.43rem;
  color: #1a1a1a;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .container .goods_info .ttl {
  font-size: 15px;
}
[data-dpr="2"] .content .container .goods_info .ttl {
  font-size: 30px;
}
[data-dpr="3"] .content .container .goods_info .ttl {
  font-size: 45px;
}
.content .container .goods_info .info {
  display: block;
  box-sizing: border-box;
  padding-top: 0.12rem;
  padding-bottom: 0.24rem;
}
.content .container .goods_info .info li {
  width: 9.14rem;
  line-height: 0.58rem;
}
.content .container .goods_info .info li .info_topic {
  width: 2.18rem;
  float: left;
}
.content .container .goods_info .info li .info_content {
  width: 6.96rem;
  float: left;
}
.content .container .goods_info .info li .clear {
  clear: both;
}
