}
.content ul .verification_code .code_input::-webkit-input-placeholder {
  color: #f0f0f0;
}
.content ul .verification_code .get_code_btn {
  background: none;
  border: none;
  background: #64bfc4;
  display: inline-block;
  height: 0.8rem;
  line-height: 0.8rem;
  text-align: center;
  width: 3.2rem;
  color: #f0f0f0;
  font-family: "Microsoft YaHei";
  border-radius: 0.1rem;
  float: right;
  margin-top: 0.225rem;
}
[data-dpr="1"] .content ul .verification_code .get_code_btn {
  font-size: 14px;
}
[data-dpr="2"] .content ul .verification_code .get_code_btn {
  font-size: 28px;
}
[data-dpr="3"] .content ul .verification_code .get_code_btn {
  font-size: 42px;
}
.content ul .pwd {
  width: 9.4rem;
