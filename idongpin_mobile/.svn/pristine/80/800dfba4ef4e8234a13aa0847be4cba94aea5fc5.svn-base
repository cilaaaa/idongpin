[data-dpr="1"] .content ul .pwd .pwd_input {
  font-size: 14px;
}
[data-dpr="2"] .content ul .pwd .pwd_input {
  font-size: 28px;
}
[data-dpr="3"] .content ul .pwd .pwd_input {
