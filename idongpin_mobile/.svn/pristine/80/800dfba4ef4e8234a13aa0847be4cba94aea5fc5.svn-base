.content .confirm {
  display: block;
  width: 7.2rem;
  height: 1.2rem;
  line-height: 1.2rem;
  margin: 0 auto;
  margin-top: 0.8rem;
  border: none;
  background: #64bfc4;
  border-radius: 0.1rem;
  color: #ffffff;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .content .confirm {
  font-size: 16px;
}
[data-dpr="2"] .content .confirm {
  font-size: 32px;
}
[data-dpr="3"] .content .confirm {
  font-size: 48px;
}
