body {
  background: #f5f5f5;
}
.header {
  width: 100%;
  height: 1.25rem;
  line-height: 1.25rem;
  text-align: center;
  background-color: #64bfc4;
  position: fixed;
  top: 0px;
  left: 0px;
}
.header .back {
  color: #ffffff;
  font-family: "Microsoft YaHei";
  position: absolute;
  left: 20px;
}
[data-dpr="1"] .header .back {
  font-size: 14px;
}
[data-dpr="2"] .header .back {
  font-size: 28px;
}
[data-dpr="3"] .header .back {
  font-size: 42px;
}
.header .page_ttl {
  color: #ffffff;
  font-family: "Microsoft YaHei";
}
[data-dpr="1"] .header .page_ttl {
  font-size: 14px;
}
[data-dpr="2"] .header .page_ttl {
  font-size: 28px;
}
[data-dpr="3"] .header .page_ttl {
  font-size: 42px;
}
.content {
  width: 100%;
  margin: 0 auto;
  margin-top: 1.25rem;
}
.content ul {
  width: 100%;
  background: #ffffff;
  border-bottom: 0.03rem solid #f0f0f0;
}
.content ul .phone_number {
  width: 9.4rem;
  height: 1.25rem;
  line-height: 1.25rem;
  margin: 0 auto;
  border-bottom: 0.03rem solid #f0f0f0;
  color: #f0f0f0;
}
.content ul .phone_number .user_icon {
  font-family: "user";
  width: 0.5rem;
  height: 0.8rem;
  line-height: 0.8rem;
  margin-top: 0.225rem;
  float: left;
}
[data-dpr="1"] .content ul .phone_number .user_icon {
  font-size: 14px;
}
[data-dpr="2"] .content ul .phone_number .user_icon {
  font-size: 28px;
}
[data-dpr="3"] .content ul .phone_number .user_icon {
  font-size: 42px;
}
.content ul .phone_number .user_input {
  border: none;
  background: none;
  pdding: 0px 20px;
  font-family: "Microsoft YaHei";
  width: 8.9rem;
  height: 0.8rem;
  float: left;
  margin-top: 0.225rem;
  line-height: 0.8rem;
  box-sizing: border-box;
  padding: 0 20px;
}
[data-dpr="1"] .content ul .phone_number .user_input {
  font-size: 14px;
}
[data-dpr="2"] .content ul .phone_number .user_input {
  font-size: 28px;
}
[data-dpr="3"] .content ul .phone_number .user_input {
  font-size: 42px;
}
.content ul .phone_number .user_input::-webkit-input-placeholder {
  color: #f0f0f0;
}
.content ul .verification_code {
  width: 9.4rem;
  height: 1.25rem;
  line-height: 1.25rem;
  margin: 0 auto;
  border-bottom: 0.03rem solid #f0f0f0;
  color: #f0f0f0;
}
.content ul .verification_code .code_icon {
  font-family: "verification_code";
  width: 0.5rem;
  height: 0.8rem;
  line-height: 0.8rem;
  margin-top: 0.225rem;
  float: left;
}
[data-dpr="1"] .content ul .verification_code .code_icon {
  font-size: 14px;
}
[data-dpr="2"] .content ul .verification_code .code_icon {
  font-size: 28px;
}
[data-dpr="3"] .content ul .verification_code .code_icon {
  font-size: 42px;
}
.content ul .verification_code .code_input {
  border: none;
  background: none;
  width: 5.7rem;
  font-family: "Microsoft YaHei";
  height: 0.8rem;
  margin-top: 0.225rem;
  line-height: 0.8rem;
  float: left;
  box-sizing: border-box;
  padding: 0 20px;
}
[data-dpr="1"] .content ul .verification_code .code_input {
