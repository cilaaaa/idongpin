.tradeMenu{
	display: block;
}

.vipMenu{
	display: block;
}

.userInfo{
	width: 762px;
	height: 120px;
	border-bottom: 1px solid #e8e8e8;
	margin-left: 28px;
}

.photo{
	width: 72px;
	height: 72px;
	float: left;
	margin-top: 24px;
	border-radius: 36px;
	-webkit-border-radius: 36px;
	-moz-border-radius: 36px;
	-ms-border-radius: 36px;
	-o-border-radius: 36px;
}

.photo img{
	width: 72px;
	height: 72px;
	border-radius: 36px;
	-webkit-border-radius: 36px;
	-moz-border-radius: 36px;
	-ms-border-radius: 36px;
	-o-border-radius: 36px;
}

.info{
	width: 560px;
	height: 72px;
	float: left;
	margin-top: 24px;
}

.useName{
	width: 560px;
	height: 36px;
	line-height: 36px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	box-sizing: border-box;
	padding: 0 15px;
}

.userType{
	color: #a5a2a2;
	font-size: 12px;
}

.orders{
	width: 560px;
	height: 24px;
	line-height: 24px;
	color: #4a4a4a;
	font-size: 12px;
	font-family: "Microsoft YaHei";
	box-sizing: border-box;
	padding: 0 15px;
}

.deal span{
	color: #e31e3c;
}

.undeal{
	margin-left: 30px;
}

.undeal span{
	color: #e31e3c;
}

.orderRelease{
	width: 128px;
	height: 32px;
	float: left;
	margin-top: 58px;
}

.releaseBtn{
	width: 124px;
	height: 32px;
	border: 2px solid #ff6c00;
	color: #ff6c00;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	line-height: 32px;
	text-align: center;
	cursor: pointer;
}

.orderInfo{
	width: 762px;
	margin-left: 28px;
	margin-top: 9px;
}

.recentOrder{
	width: 762px;
	height: 32px;
	box-sizing: border-box;
	padding: 0px 10px;
	line-height: 32px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.recentOrder img{
	margin-left: 16px;
}

.orderTab{
	width: 760px;
	border-collapse: collapse;
	border: 1px solid #e8e8e8;
}

.th{
	width: 760px;
	height: 34px;
	line-height: 24px;
	background: #f7f7f7;
	box-sizing: border-box;
	padding: 0 28px;
}

.th .orderID{
	float: left;
	width: 188px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .goodsName{
	float: left;
	width: 136px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .price{
	float: left;
	width: 102px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .count{
	float: left;
	width: 76px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .freight{
	float: left;
	width: 100px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";	
}

.th .operation{
	float: left;
	width: 98px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.tr{
	width: 704px;
	height: 46px;
	margin: 0 auto;
	border-bottom: 1px solid #e8e8e8;
}

.tr .orderID{
	float: left;
	width: 188px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .goodsName{
	float: left;
	width: 136px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .price{
	float: left;
	width: 102px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .count{
	float: left;
	width: 76px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .freight{
	float: left;
	width: 100px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";	
}

.tr .operation{
	float: left;
	width: 98px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.cancel{
	color: #4a4a4a;
	line-height: 26px;
	cursor: pointer;
	text-decoration: underline;
}

.cancel:hover{
	color: #ff6c00;
}

.spacing{
	padding: 0 15px;
}

.finish{
	color: #4a4a4a;
	line-height: 24px;
	cursor: pointer;
	text-decoration: underline;
}

.finish:hover{
	color: #ff6c00;
}