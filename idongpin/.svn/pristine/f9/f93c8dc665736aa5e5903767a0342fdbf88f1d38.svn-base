/*弹窗*/
.modifiedMode{
	display: none;
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 10000;
	background: rgba(0,0,0,0.2);
	top: 0px;
	left: 0px;
}

.modified{
	width: 620px;
	margin: 0 auto;
	border: 1px solid #e8e8e8;
	background: #ffffff;
}
.modified .orderInfo{
	width: auto;
	height: auto;
}
.modified .orderInfo1{
	width: auto;
}
.orderInfoTtl{
	width: 620px;
	height: 32px;
	line-height: 32px;
	border-bottom: 1px solid #e8e8e8;
	box-sizing: border-box;
	padding: 0 20px;
	position: relative;
	border-bottom: 1px solid #e8e8e8;
}

.orderOpen{
	display: inline-block;
	height: 32px;
	line-height: 32px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
}


.orderInfoTtl img{
	width: 18px;
	height: 18px;
	position: absolute;
	right: 7px;
	top: 7px;
}

.modified .info{
	width: 620px;
	margin-top: 0px;
	height: auto;
	float: none;
}

.topInfo{
	width: 620px;
	box-sizing: border-box;
	padding: 0 15px;
	margin-top: 22px;
}

.topInfo .form-group{
	display: inline-block;
	width: 45%;
	margin-bottom: 5px;
	float:left;
}

.topInfo .right-input{
	float:right;
}

.infoTopic{
	display: inline-block;
	float: left;
	padding-top: 7px;
	margin-top: 0;
	margin-bottom: 0;
	width: 45%;
	color: #4a4a4a;
	/*font-weight: bold;*/
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.infoTopic:after,.topInfo .form-group .form-control:after{
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.infoTopic img{
	width: 0px;
}

.has-error,.has-error .infoTopic{
	color: #a94442;
}

.topInfo .form-group .form-control{
	width: 55%;
	float: left;
	display: inline-block;
}

.modified .infoInput{
	display: block;
	width: 100%;
	height: 34px;
	padding: 6px 12px;
	font-size: 14px;
	line-height: 1.428571429;
	color: #555;
	vertical-align: middle;
	background-color: #fff;
	background-image: none;
	border: 1px solid #ccc;
	/*border-radius: 4px;*/
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
	-webkit-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
	transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
	font-family: "Microsoft YaHei";
    color: #4a4a4a;
    box-shadow: 4px 4px 14px #f6f6f6 inset;
    -webkit-box-shadow: 4px 4px 14px #f6f6f6 inset;
    -moz-box-shadow: 4px 4px 14px #f6f6f6 inset;
    -ms-box-shadow: 4px 4px 14px #f6f6f6 inset;
    -o-box-shadow: 4px 4px 14px #f6f6f6 inset;
}

.has-error .infoInput{
	border-color: #a94442;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075)
}


.help-block{
	display: block;
	font-size:12px;
	margin-top: 5px;
	margin-bottom:5px;
}

