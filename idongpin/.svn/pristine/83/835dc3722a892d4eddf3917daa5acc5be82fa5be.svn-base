	font-family: "Microsoft YaHei";
	float: left;
}

.space{
	margin: 0 6px;
	height: 30px;
	line-height: 30px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}

.conditionSearch{
	margin-left: 124px;
	cursor: pointer;
}

.conditionSearchBtn{
	display: inline-block;
	width: 98px;
	height: 30px;
	border: none;
	background: none;
	background:  #e31e3c;
	color: #ffffff;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	line-height: 30px;
	text-align: center;
	cursor: pointer;
}

.selectOrderStatus{
	width: 762px;
	height: 32px;
	line-height: 32px;
}

.selectOrderStatus .infoTtl{
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.status{
	display: inline-block;
	width: 70px;
	height: 24px; 
	border-radius: 5px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	text-align: center;
	line-height: 24px;
	margin-left: 8px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	cursor: pointer;
}

.statusActive{
	color: #ffffff;
	background-color: #ff6c00;
}

.afterThreeMonth{
	width: 762px;
	background: #ffffff;
	margin-top: 3px;
	margin-left: 28px;
}

.beforeThreeMonth{
	width: 762px;
	background: #ffffff;
	margin-top: 3px;
	display: none;
	margin-left: 28px;
}

.orderTab{
	width: 760px;
	border-collapse: collapse;
	border: 1px solid #e8e8e8;
}

.th{
