	height: 34px;
	line-height: 24px;
	background: #f7f7f7;
	box-sizing: border-box;
	padding: 0 28px;
}

.th .orderID{
	float: left;
	width: 188px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .goodsName{
	float: left;
	width: 136px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .price{
	float: left;
	width: 102px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .count{
	float: left;
	width: 76px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .freight{
	float: left;
