	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";	
}

.th .operation{
	float: left;
	width: 98px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.tr{
	width: 704px;
	height: 46px;
	margin: 0 auto;
	border-bottom: 1px solid #e8e8e8;
}

.tr .orderID{
	float: left;
	width: 188px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
	cursor: pointer;
}

.tr .orderID:hover{
	color: #ff6c00;
}

.tr .goodsName{
	float: left;
	width: 136px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.tr .price{
	float: left;
	width: 102px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .count{
	float: left;
	width: 76px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .freight{
	float: left;
	width: 100px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";	
}

.tr .operation{
	float: left;
	width: 98px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.cancel{
	color: #4a4a4a;
	line-height: 26px;
	cursor: pointer;
	text-decoration: underline;
}

.cancel:hover{
	color: #ff6c00;
}

.spacing{
	padding: 0 10px;
}

.finish{
	color: #4a4a4a;
	line-height: 24px;
}

/*垂直水平居中*/
.modeTable{
	display: table;
	width: 100%;
	height: 100%;
}
.modeTableCell{
	display:table-cell;
	vertical-align:middle;
	height: 100px;
}

.modified{
	width: 620px;
    margin: 0 auto;
    border: 1px solid #e8e8e8;
    background: #ffffff;
}

.modified{
	width: 620px;
	margin: 0 auto;
	border: 1px solid #e8e8e8;
	background: #ffffff;
}
.modified .orderInfo{
	width: auto;
	height: auto;
}
.modified .orderInfo1{
	width: auto;
}
.orderInfoTtl{
	width: 620px;
	height: 46px;
	line-height: 36px;
	box-sizing: border-box;
	padding: 0 50px;
	position: relative;
	border-bottom: 6px solid #f7f7f7;
}

.orderOpen{
	display: inline-block;
	height: 36px;
	line-height: 36px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
}


.orderInfoTtl img{
	width: 18px;
	height: 18px;
	position: absolute;
	right: 9px;
	top: 9px;
}

.modified .info{
	width: 620px;
	margin-top: 0px;
	height: auto;
	float: none;
	padding-bottom: 15px;
}

.topInfo{
	width: 620px;
	box-sizing: border-box;
	padding: 0 50px;
	/*margin-top: 22px;*/
}

.topInfo .form-group{
	display: inline-block;
	/*width: 45%;*/
	/*margin-bottom: 5px;*/
	/*float:left;*/
}


.orderMode{
	display: none;
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 10000;
	background: rgba(0,0,0,0.2);
	top: 0px;
	left: 0px;
}
.orderMode .info .topInfo{
	margin-bottom: 22px;
}
/*.orderMode .info .topInfo li{
	height: 35px;
}*/

.orderMode .info .topInfo .form-group{
	width: 50%;
}
.form-group-name{
	display: inline-block;
	/*height: 35px;*/
	/*line-height: 35px;*/
	width: 45%;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}
.form-group-text{
	display: inline-block;
	/*height: 35px;*/
	/*line-height: 35px;*/
	width: 55%;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}

.orderStatus{
	width: 180px;
	float: right;
}

.orderMode .info .topInfo .product_name,.orderMode .info .topInfo .supply_company,
.orderMode .info .topInfo .product_freight,.orderMode .info .topInfo .product_total,
.orderMode .info .topInfo .recive_address{
	width: auto;
	display: block;
	box-sizing: border-box;
	padding: 13px 0px;
	border-bottom: 1px solid #f7f7f7;
}

.orderMode .info .topInfo .recive_address{
	border: none;
}

.supply_company .supply_phone,.product_name .product_price{
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft Yahei";
	float: right;
}

/*.product_name .product_price{
	float: right;
}*/

.supply_company .form-group-name,.product_name .form-group-name,
.product_freight .form-group-name,.product_total .form-group-name,
.recive_address .form-group-name{
	width: 152px;
}

.supply_company .form-group-text,.product_name .form-group-text{
	width: 276px;
}

.product_name .product_amout{
	float: right;
}

.product_name .product_amout .product_price{
	display: block;
}

.product_name .product_amout .amount{
	display: block;
	color: #808282;
	font-size: 12px;
	font-family: "Microsoft Yahei";
}

.product_freight .form-group-text,.product_total .form-group-text{
	width: 152px;
	float: right;
	text-align: right;
}

.product_total .form-group-text{
	color: #ff6c00;
	font-size: 20px;
}

.recive_address .form-group-text{
	width: auto;
}

.hr{
	width: 100%;
	height: 6px;
	background-color: #f7f7f7;
}

.orderMode .info .topInfo .order_number,.orderMode .info .topInfo .orderStatus{
	box-sizing: border-box;
	padding: 17px 0px;
}

.order_number .form-group-name{
	width: 85px;
}

.orderMode .info .topInfo .orderStatus{
	width: auto;
	float: right;
}

.orderStatus .form-group-name{
	width: 85px;
	text-align: right
}

.orderStatus .form-group-text{
	width: auto;
	color: #ff6c00;
}

/*.orderMode .info .topInfo .product_name{
	margin-top: 20px;
	display: block;
	width: auto;
}*/

.orderMode .info .topInfo .order_create,.orderMode .info .topInfo .order_confirm,
.orderMode .info .topInfo .order_finish{
	box-sizing: border-box;
	padding: 4px 0px;
}

.order_create .form-group-name,.order_confirm .form-group-name,.order_finish .form-group-name{
	width: 85px;
	color: #747575;
}

.order_create .form-group-text,.order_confirm .form-group-text,.order_finish .form-group-text{
	width: auto;
	color: #747575;
}

.afterThreeMonth{
	border: 1px solid #e8e8e8;
	border-collapse:collapse;
}
.afterThreeMonth thead{
	background-color: #f7f7f7;
}
.afterThreeMonth thead td{
	height: 34px;
	line-height: 34px;
	color:#4a4a4a;
	border:none;
	padding-left: 10px;
	font-size: 14px;
	font-family: "Microsoft Yahei";
}
.afterThreeMonth tbody td{
	color:#4a4a4a;
	border:none;
	padding-left: 5px;
	font-size: 14px;
	height: 40px;
	border-top: 1px solid #f7f7f7;
	font-family: "Microsoft Yahei";
}
.afterThreeMonth tbody td .sure{
	text-decoration: underline;
}
.afterThreeMonth tbody td .sure:hover{
	cursor: pointer;
	color:rgb(255, 108, 0);
}
.afterThreeMonth tbody tr .orderID:hover{
	cursor: pointer;
	color:rgb(255, 108, 0);
}
