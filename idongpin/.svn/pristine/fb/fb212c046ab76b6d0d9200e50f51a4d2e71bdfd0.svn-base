.customerService{
	position: fixed;
	right: 0px;
	bottom: 20%;
	width: 48px;
	height: 192px;
	z-index: 999;
}

.customerService img{
	width: 48px;
	height: 192px;
}

.viwepager{
	position: absolute;
	top: 147px;
	width: 100%;
	height: 372px;
}

.ad{
	display: none;
	width: 240px;
	height: 372px;
	position: absolute;
	right: 0;
	z-index: 99;
}

.ad img{
	opacity: 0.6;
}

.brandPromise{
	width: 950px;
	height: 100px;
	margin: 0 auto;
	border-bottom: 1px solid #e8e8e8;
}


.brandPromise img{
	width: 100%;
	height: 100%;
}

.zoushiPic{
	margin: 0 auto;
	margin-top: 40px;
	width: 950px;
	height: 260px;
	text-align: center;
	overflow: hidden;
}

.zoushiPic img{
	width: 946px;
	height: 260px;
}

.goods{
	width: 980px;
	margin: 0 auto;
	padding-top: 33px;
	box-shadow: 0 0 20px #f2f6f7;
	-webkit-box-shadow: 0 0 40px #f2f6f7;
	-moz-box-shadow: 0 0 40px #f2f6f7;
	-ms-box-shadow: 0 0 40px #f2f6f7;
	-o-box-shadow: 0 0 40px #f2f6f7;
}

.goodsTeme{
	width: 910px;
	height: 44px;
	margin: 0 auto;
	border-bottom: 3px solid #ff6c00;
	padding: 10px 20px;
	padding-bottom: 0px;
}

.recomend{
	float: left;
	margin-top: 13px;
}

.hostGoods{
	color: #ff6c00;
	font-size: 22px;
	font-family: "Microsoft YaHei";
	line-height: 22px;
}

.spot,.purpose{
	color: #a1a1a1;
	font-size: 16px;
	font-family: "Microsoft YaHei";
}

.sort{
	float: right;
	margin-top: 13px;
}

.sortGoods{
	float: left;
	line-height: 31px;
}

.option{
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
	height: 31px;
	padding: 0 7px;
}

.option:hover{
	cursor: pointer;
}

.optionActive{
	color: #ff6c00;
}

.choice{
	height: 26px
}

.choiceIcon{
	height: 5px;
	text-align: center;	
}

.choiceIcon img{
	display: none;
	width: 10px;
	height: 5px;
	float: left;
}

.sort span{
	float: left;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.sort .more{
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	margin-left: 20px;
	line-height: 31px;
}

.content{
	width: 980px;
	margin: 0 auto;
	zoom: 1;
}

.goodsMode{
	display: none;
	width: 950px;
	margin: 0 auto;
	border-top: none;
	overflow: auto;
	zoom: 1;
	box-sizing: border-box;
	border-top: 1px solid #e8e8e8;
	border-left: 1px solid #e8e8e8;
}

.modeActive{
	display: block;
}

.content li{
	position: relative;
	width: 189.8px;
	height: 333.2px;
	float: left;
	box-sizing: border-box;
	padding: 20px;
	cursor: pointer;
	border-bottom: 1px solid #e8e8e8;
	border-right: 1px solid #e8e8e8;
}


.hotGoodsSign{
	position: absolute;
	top: 5px;
	right: 5px;
}
.hotGoodsSign img{
	width: 38px;
	height: 24px;
	right: 0;
}
.goodsImg{
	width: 149.8px;
	height: 149.8px;
	overflow: hidden;
}

.goodsImg img{
	width: 100%;
	height: 100%;
}

.goodsIntroduce{
	width: 149.8px;
	height: 33px;
	margin-top: 20px;
}

.goodsName{
	width: 149.8px;
	height: 15px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.goodsKind{
	width: 149.8px;
	height: 13px;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft YaHei";
	margin-top: 5px;
}

.goodsInfo{
	width: 149.8px;
	height: 39px;
	margin-top: 20px;
}

.priceNow{
	width: 149.8px;
	height: 21px;
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.now{
	color: #ff6c00;
	font-size: 22px;
	font-family: "Microsoft YaHei";
}

.price{
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.regularPrice{
	width: 148px;
	height: 12px;
	margin-top: 6px;
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	text-decoration: line-through;
}

.proUnitPrice{
	float: right;
}

.belong{
	width: 148px;
	height: 12px;
	margin-top: 10px;
}

.belong img{
	float: left;
	width: 13px;
	height: 13px;
	margin-top: -1px;
}

.company{
	float: left;
	margin-left: 10px;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft YaHei";
}

.address{
	float: right;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft YaHei";
}

.productionBusiness{
	width: 980px;
	margin: 0 auto;
	padding-top: 25px;
	padding-bottom: 20px;
}

.topTtlBar{
	width: 950px;
	height: 42px;
	margin: 0 auto;
}

 .topTtlBarLeft{
	width: 468px;
	height: 42px;
	float: left;
	margin-left: 3px;
	border-bottom: 3px solid #ff6c00;
}


.topTtlBarRight{
	width: 468px;
	height: 42px;
	line-height: 42px;
	float: right;
	border-bottom: 3px solid #ffa800;
}


.superior{
	color: #ff6c00;
	font-size: 22px;
	font-family: "Microsoft YaHei";
	line-height: 22px;
}

.superiorRight{
	color: #ffa800;
	font-size: 22px;
	font-family: "Microsoft YaHei";
	line-height: 22px;
}

.showBusiness{
	width: 949px;
	zoom: 1;
	margin: 0 auto;
}

.showBusiness:after{
	clear:both;
	content:'';
	display:block;
}

.showBusinessLeft{
	width: 468px;
	height: 157px;
	float: left;
	margin-left: 3px;
}

.showBusinessRight{
	width: 468px;
	height: 157px;
	float: right;
}

.business{
	cursor: pointer;
}

.business_1{
	width: 227px;
	height: 157px;
	float: left;
}

.business_2{
	width: 227px;
	height: 157px;
	float: right;
}

.showBusinessLeft .business img{
	width: 100%;
	height: 90px;
	margin-top: 12px;
	border: 1px solid #e8e8e8;
}

.showBusinessRight .business img{
	width: 100%;
	height: 90px;
	margin-top: 12px;
}

.business div{
	width: 227px;
	height: 55px;
	line-height: 55px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

/*promise*/
.promise{
	width: 980px;
	margin: 0 auto;
}

.brandPromise1, .brandPromise2, .brandPromise3, .brandPromise4 {
    float: left;
    width: 25%;
    height: 100px;
}



.pInitAll{
	width: 170px;
	color: #5f5f5f;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}
.pInitAllTitle{
	height: 40px;
}
.pInitAllValue{
	text-align: left;
}
.pInitAll img{
	float: left;
	width: 40px;
	height: 40px;
}

.pInitAll span{
	display: inline-block;
	margin-left: 5px;
	float: left;
	height: 40px;
	line-height: 40px;
. color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
}
.pInitTop{
	box-sizing: border-box;
	height: 117px;
	padding-top: 26px;
	margin: 0 auto;
}
/*//中间三个元素*/
.pInitCenter {
	box-sizing: border-box;
	height: 177px;
	padding-left: 45px;
	padding-right: 45px;
}
.pInitCenter .pInitCenterL{
	float: left;
}
.pInitCenter .pInitCenterC{
	float: left;
	box-sizing: border-box;
	padding-top: 88px;
	width: 550px;
	text-align: center;
}
.pInitCenterC img{
	width: 298px;
	height: 64px;
}

/*底部三个元素*/
.pInitBottom {
	box-sizing: border-box;
	height: 200px;
	padding-left: 45px;
	padding-right: 45px;
}
.pInitBottom  .pInitBottomL{
	float: left;
}
.pInitBottom  .pInitBottomC{
	float: left;
	box-sizing: border-box;
	padding-top: 90px;
	width: 550px;
	text-align: center;
}
.pInitBottom  .pInitBottomCC{
	margin: 0 auto;
}


/*promise*/

.advantage{
	width: 950px;
	height: 47px;
	line-height: 44px;
	margin: 0 auto;
	margin-top: 25px;
	border-bottom: 3px solid #ff6c00;
	box-sizing: border-box;
	padding: 0 20px;
}

.learnMore{
	position: absolute;
	left: 11px;
	top: 229px;
	width: 96px;
	height: 36px;
	line-height: 32px;
	text-align: center;
	border: 2px solid #ff6c00;
	color: #ff6c00;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}