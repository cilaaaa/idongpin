	color:#333333;
}
 .list >span{
	display: inline-block;
	height: 45px;
	line-height: 45px;
	float: left;
	font-size: 14px;
	font-family: '微软雅黑';
	 color:#333333;
}
select{
	/*Chrome和Firefox里面的边框是不一样的，所以复写了一下*/
	border: solid 1px #e9ebe9;
	/*很关键：将默认的select选择框样式清除*/
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	/*在选择框的最右侧中间显示小箭头图片*/
	background: url("http://www.idongpin.com.cn/images/userCenter/arrow.png") no-repeat scroll right center transparent;
	/*为下拉小箭头留出一点位置，避免被文字覆盖*/

}
/*清除ie的默认选择框样式清除，隐藏下拉箭头*/
select::-ms-expand { display: none; }

.list input,.list  select{
	font-size: 14px;
	font-family: '微软雅黑';
	color:#333333;
	height: 28px;
	width: 175px;
	margin: 7.5px 0;
	padding-left: 5px;
	border:1px solid #a1a1a1;
	/*border-radius: 5px;*/
	display: inline-block;
}

 .list select{
	display: none;
	 width: 182px;
	 box-shadow: 4px 4px 14px #f6f6f6 inset;
	 -webkit-box-shadow: 4px 4px 14px #f6f6f6 inset;
	 -moz-box-shadow: 4px 4px 14px #f6f6f6 inset;
	 -ms-box-shadow: 4px 4px 14px #f6f6f6 inset;
	 -o-box-shadow: 4px 4px 14px #f6f6f6 inset;
}

.list .form-control{
	display: none;
	float: left;
}
.list .form-control input{
	display: block;
}
.list .form-control .help-block{
	display: none;
}
.list .form-control .help-block strong{
	display: inline-block;
	width: 175px;
	height: 16px;
	white-space:nowrap;
	text-overflow:ellipsis;
	-o-text-overflow:ellipsis;
	overflow: hidden;
	font-size: 12px;
	font-family: "Microsoft Yahei";
}
.has-error,.has-error label,.has-error .form-control .help-block{
	color: #a94442;
}
.has-error   input{
	border:1px solid #a94442;
}
.has-error .form-control .help-block{
	display: block;
}