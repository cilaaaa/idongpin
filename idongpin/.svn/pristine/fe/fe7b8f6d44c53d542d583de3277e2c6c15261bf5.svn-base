*{
    margin: 0;
    padding: 0;
    outline: 0;
}
.clear{
    clear: both;
}
.displayNone{
    display: none;
}
.displayBlock{
    display: block;
}
.infoMain{
    box-sizing: border-box;
    padding-left: 28px;
    padding-right: 15px;
}
.infoTags{
    background: #FFFFFF;
    height: 122px;
    padding-top: 20px;
    box-sizing: border-box;
}
.infoTagsName{
    height: 31px;
    color: #3a3b3b;
    font-size: 14px;
    font-family: "Microsoft YaHei";
}
.infoLevel{
    height: 36px;
    line-height: 36px;
    padding-left: 15px;
}
.tagsName{
    color: #585858;
    font-size: 14px;
    font-family: "Microsoft YaHei";
}
.tagsText{
    color: #e31e3c;
    font-size: 14px;
    font-family: "Microsoft YaHei";
}

.infoDetails{
    padding: 0 15px ;
    background: #FFFFFF;
}
.head{
}
.headImg{
    float: left;
}
.head img{
    width: 64px;
    height: 64px;
    cursor: pointer;
}
.head span{
    padding-left: 5px;
    color: #333333;
    font-size: 14px;
    font-family: "Microsoft YaHei";
    cursor: pointer;
}
.Certification{
    height: 20px;
    float: left;
}
.details{

}

.Certification .a{
    margin-right: 10px;
    font-size: 14px;
    color: #4a4a4a;
    font-family: "Microsoft Yahei";

}
.Certification .b{
    color: #333333;
    font-size: 14px;
    font-family: "Microsoft Yahei";
}


.detailsItem{
    height: 32px;
    line-height: 32px;
}
.detailsItem .detailsItemName{
    font-size: 14px;
    font-family: "Microsoft Yahei";
    color: #333333;
}
.detailsItem .detailsItemText{
    display: inline-block;
    width: 250px;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    color: #333333;
}

.detailsItemA{
    color:#e02341 ;
    cursor: pointer;
    text-decoration: underline;
}
