*{
    margin: 0;
    padding: 0;
    outline: 0;
}
.clear{
    clear: both;
}
.displayNone{
    display: none;
}
.displayBlock{
    display: block;
}
.infoMain{
    box-sizing: border-box;
    margin-left: 28px;
    margin-right: 15px;
}
.infoTags{
    background: #FFFFFF;
    height: 115px;
    padding-top: 20px;
    box-sizing: border-box;
}
.countName{
    color: #3a3b3b;
    font-size: 14px;
    font-family: "Microsoft YaHei";
    height: 31px;
}
.countOpera{
    height: 35px;
    line-height: 35px;
    padding-left: 15px;
}
 .tagsName{
    margin-right: 30px;
     padding: 5px 0;
    color: #3a3b3b;
    font-size: 14px;
    font-family: "Microsoft YaHei";
     cursor: pointer;
}
 .tagsText{
    padding: 5px 0;
    color: #3a3b3b;
    font-size: 14px;
    font-family: "Microsoft YaHei";
     cursor: pointer;
}

.redSpan{
    border-bottom: 3px solid #ff6c00;
}
.infoDetails{
    padding: 0 15px ;
    background: #FFFFFF;
}
.head{
    width: 102px;
    float: left;
}
.head img{
    width: 72px;
    height: 72px;
    cursor: pointer;
}
.head a{
    display: block;
    width: 72px;
    text-align: center;
    padding-top: 7px;
    color: #585858;
    font-size: 14px;
    font-family: "Microsoft YaHei";
    cursor: pointer;
    text-decoration: underline;
}

.details{
    float: left;
}
.detailsC{
    float: left;
}
.Certification{
    height: 42px;
    line-height: 42px;
}
.Certification .a{
    margin-right: 10px;
    font-size: 14px;
    color: #3a3b3b;
    font-family: "Microsoft Yahei";

}
.Certification .b{
    color: #868686;
    font-size: 14px;
    font-family: "Microsoft Yahei";
}


.detailsItem{
    height: 46px;
}

.displayT{

}
.displayF{
    display: none;
}
.displayF input{
    box-sizing: border-box;
    padding-left: 10px;
    margin-left: 10px;
    width: 200px;
    height: 30px;
    border: 1px solid rgb(204, 204, 204);
}

.detailsItem .detailsItemName{
     display: inline-block;
     height:46px;
     line-height: 46px;
     font-size: 14px;
     font-family: "Microsoft Yahei";
     color: #3a3b3b;
 }
.detailsItem .detailsItemText{
    font-size: 14px;
    font-family: "Microsoft Yahei";
    color: #3a3b3b;
    padding-left: 10px;
}
.textTop{
    position: relative;
    top: 2px;
    margin-right: 5px;
}
.opera{
    height: 47px;
    line-height: 47px;
    padding-left: 240px;
    margin-top: 172px;
}

.opera span{
    display: inline-block;
    box-sizing: border-box;
    cursor: pointer;
    width: 100px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    margin-right: 10px;
    background-color:#ff6c00;
    border: 2px;
    border-radius: 4px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    -ms-border-radius: 4px;
    -o-border-radius: 4px;
    color: #FFFFFF;
    font-family: "Microsoft Yahei";
    font-size: 14px;
}


/*弹窗*/

.itemLeft{
    height: 46px;
    float: left;
}
.itemLeft span{
    display: inline-block;
    height: 46px;
    line-height: 46px;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    color: #3a3b3b;
}
.typeSelect{
    height: 46px;
    width: 200px;
    float: left;
    margin-left: 10px;
}

.typeHeader{
    box-sizing: border-box;
    height:30px;
    line-height: 30px;
    margin:  8px 0;
    border: 1px solid rgb(204, 204, 204);
    cursor: pointer;
}

.typeHeader >div{
    box-sizing: border-box;
    padding-left: 10px;
    width: 170px;
    height: 28px;
    line-height: 28px;
    float: left;
    white-space:nowrap;
    text-overflow:ellipsis;
    -o-text-overflow:ellipsis;
    overflow: hidden;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    color: #3a3b3b;

}
.typeHeader >div span{
    margin-right: 5px;
}

.typeHeader img{
    width: 28px;
    height: 28px;
    float: left;
}
.personalType{
    position: relative;
    display: none;
    background-color: #ffffff;
    margin-top: -8px;
    z-index: 999;
    border: 1px solid rgb(204, 204, 204);
    font-size: 14px;
    font-family: "Microsoft Yahei";
    color: #3a3b3b;
}

.personalType li{
    height: 30px;
    line-height: 30px;
    padding-left: 10px;
    cursor: pointer;
    /*border-top:1px solid rgb(204, 204, 204);*/
}
.personalType li:hover{
    background: #f2f2f2;
}

.itemInput{
    height: 30px;
    width: 200px;
    border: 1px solid rgb(204, 204, 204);

    color: #4a4a4a;
    font-size: 14px;
    font-family: "Microsoft Yahei";
    padding-left: 10px;
    box-sizing: border-box;
}

.industryCheck{
    float: right;
    width: 13px;
    height: 13px;
    margin-top: 8.5px;
    margin-right: 10px;
    cursor: pointer;
}
/*主营行业样式*/
.industryConfirm,.industryCancel{
    float: left;
    text-align: center;
    height: 30px;
    line-height: 30px;
    width: 50%;
    background-color: #ffffff;
    margin: 5px 0;
}
.industryConfirm span,.industryCancel span{
    display: inline-block;
    width: 60px;
    height: 20px;
    line-height: 20px;
    background-color: #ff6c00;
    border-radius: 3px;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    -ms-border-radius: 3px;
    -o-border-radius: 3px;
    color: #ffffff;
    text-align: center;
    cursor: pointer;
}
.productSelect .personalType{
    z-index: 990;
}
/*弹窗*/

.w2{
    letter-spacing:2em; /*如果需要y个字两端对齐，则为(x-y)/(y-1),这里是（4-2）/(2-1)=2em */
    margin-right:-1.5em; /*同上*/
}
.w3{
    letter-spacing:0.5em; /*如果需要y个字两端对齐，则为(x-y)/(y-1),这里是（4-3）/(3-1)=0.5em */
    /*margin-right:-1.5em; !*同上*!*/
}
.w4{
    margin-right: 0.5em;
}

.down{
    width: 36px;
    height: 35px;
    float: right;
    background: url("../../images/auth/down.png");
    background-size: 100%;
}
.tt{
    transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
    -moz-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -o-transform: rotate(180deg);
}