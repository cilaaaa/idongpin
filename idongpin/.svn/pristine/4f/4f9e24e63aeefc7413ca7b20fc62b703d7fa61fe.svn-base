
input[type="text"]{
	box-shadow: 4px 4px 14px #f6f6f6 inset;
	-webkit-box-shadow: 4px 4px 14px #f6f6f6 inset;
	-moz-box-shadow: 4px 4px 14px #f6f6f6 inset;
	-ms-box-shadow: 4px 4px 14px #f6f6f6 inset;
	-o-box-shadow: 4px 4px 14px #f6f6f6 inset;
}

.tradeMenu{
	display: block;
}

.pageNav{
	width: 762px;
	height: 46px;
	line-height: 46px;
	margin-top: 14px;
	color:#4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.topMode{
	width: 762px;
	height: 237px;
	background: #ffffff;
	margin-left: 28px;
}

.selectOrderTime{
	width: 762px;
	height: 31px;
	border-bottom: 1px solid #e8e8e8;
}

.selectDate{
	display: inline-block;
	height: 30px;
	line-height: 32px;
	margin: 0px 10px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	cursor: pointer;
}

.selectDateActive{
	border-bottom: 2px solid #ff6c00;
}

.orderInfoSearch{
	width: 762px;
	height: 94px;
	margin-top: 30px;
}

.conditions{
	width: 762px;
	height: 30px;
	margin-bottom: 17px;	
}

.condition_1{
	width: 238px;
	height: 30px;
	line-height: 30px;
	display: inline-block;
	margin-right: 11px;
	float: left;
}

.condition_2{
	width: 253px;
	height: 30px;
	line-height: 30px;
	display: inline-block;
	margin-right: 9px;
	float: left;
}

.condition_3{
