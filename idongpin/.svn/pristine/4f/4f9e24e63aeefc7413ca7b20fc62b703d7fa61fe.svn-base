}

.condition_1 .infoTtl{
	display: inline-block;
	width: 59px;
	height: 30px;
	line-height: 30px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}

.condition_2 .infoTtl,.condition_3 .infoTtl{
	display: inline-block;
	width: 73px;
	height: 30px;
	line-height: 30px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}

.infoVal{
	display: inline-block;
	width: 176px;
	height: 28px;
	line-height: 28px;
	text-align: left;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	border: 1px solid #e8e8e8;
	float: left;
}

.condition_4 .infoTtl{
	display: inline-block;
	width: 74px;
	height: 30px;
	line-height: 30px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}

.space{
	margin: 0 6px;
	height: 30px;
	line-height: 30px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	float: left;
}

.conditionSearch{
	margin-left: 124px;
	cursor: pointer;
}

.conditionSearchBtn{
	display: inline-block;
	width: 98px;
	height: 30px;
	border: none;
	background: none;
	background:  #e31e3c;
	color: #ffffff;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	line-height: 30px;
	text-align: center;
	cursor: pointer;
}

.selectOrderStatus{
	width: 762px;
	height: 32px;
	line-height: 32px;
}

.selectOrderStatus .infoTtl{
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.status{
	display: inline-block;
	width: 70px;
	height: 24px; 
	border-radius: 5px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	-ms-border-radius: 3px;
	-o-border-radius: 3px;
	text-align: center;
	line-height: 24px;
	margin-left: 8px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	cursor: pointer;
}

.statusActive{
	color: #ffffff;
	background-color: #ff6c00;
}

.afterThreeMonth{
	width: 762px;
	background: #ffffff;
	margin-top: 3px;
	margin-left: 28px;
}

.beforeThreeMonth{
	width: 762px;
	background: #ffffff;
	margin-top: 3px;
	display: none;
	margin-left: 28px;
}

.orderTab{
	width: 760px;
	border-collapse: collapse;
	border: 1px solid #e8e8e8;
}

.th{
	width: 760px;
	height: 34px;
	line-height: 24px;
	background: #f7f7f7;
	box-sizing: border-box;
	padding: 0 28px;
}

.th .goodsName{
	float: left;
	width: 150px;
	height: 32px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .merchant{
	float: left;
	height: 32px;
	width: 245px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .offer{
	float: left;
	width: 106px;
	height: 32px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.th .freight{
	float: left;
	width: 102px;
	height: 32px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";	
}

.th .operation{
	float: left;
	width: 98px;
	height: 32px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 32px;
	font-family: "Microsoft YaHei";
}

.tr{
	width: 704px;
	height: 46px;
	margin: 0 auto;
	border-bottom: 1px solid #e8e8e8;
}


.tr .goodsName{
	float: left;
	width: 150px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .merchant{
	float: left;
	width: 245px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.merchant img{
	transform: rotate(-90deg);
	-webkit-transform: rotate(-90deg);
	-moz-transform: rotate(-90deg);
	-ms-transform: rotate(-90deg);
	-o-transform: rotate(-90deg);
}

.tr .offer{
	float: left;
	width: 106px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.tr .freight{
	float: left;
	width: 102px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";	
}

.tr .operation{
	float: left;
	width: 98px;
	height: 46px;
	color: #4a4a4a;
	font-size: 14px;
	line-height: 46px;
	font-family: "Microsoft YaHei";
}

.cancel{
	color: #4a4a4a;
	line-height: 26px;
	cursor: pointer;
	text-decoration: underline;
}

.cancel:hover{
	color: #ff6c00;
}

.spacing{
	padding: 0 15px;
}

.finish{
	color: #4a4a4a;
	line-height: 24px;
	cursor: pointer;
	text-decoration: underline;
}

.finish:hover{
	color: #ff6c00;
}

.detailMode{
	display: none;
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 999;
	background: rgba(0,0,0,0.2);
	top: 0px;
	left: 0px;
}

.detail{
	width: 722px;
	height: 562px;
	margin: 0 auto;
	border: 1px solid #e8e8e8;
	background: #ffffff;
}

.orderInfo{
	width: 720px;
	height: 400px;
}

.orderInfoTtl{
	width: 720px;
	height: 32px;
	line-height: 32px;
	border-bottom: 1px solid #e8e8e8;
	box-sizing: border-box;
	padding: 0 20px;
	position: relative;
}

.askPriceOrder{
	display: inline-block;
	height: 32px;
	line-height: 32px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
}

.publishDate{
	display: inline-block;
	height: 32px;
	line-height: 32px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	margin-left: 20px;
}

.orderInfoTtl img{
	width: 18px;
	height: 18px;
	position: absolute;
	right: 7px;
	top: 7px;
}

.info{
	width: 720px;
	height: 340px;
}

.topInfo{
	width: 720px;
	height: 120px;
	border-bottom: 1px solid #e8e8e8;
	box-sizing: border-box;
	padding: 0 20px;
}

.topInfo div{
	width: 680px;
	height: 40px;
	line-height: 40px;
}

.infoTopic{
	display: inline-block;
	width: 100px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.infoInput{
	display: inline-block;
	height: 28px;
	width: 200px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.gap{
	box-sizing: border-box;
	padding: 0 75px;
}

.special{
	margin-left: 66px;
}

.bottomInfo{
	width: 720px;
	height: 220px;
	box-sizing: border-box;
	padding: 0 20px;
}

.infoBar{
	width: 680px;
	height: 40px;
	line-height: 40px;
}

.other{
	float: left;
}

.otherNeed{
	display: inline-block;
	width: 566px;
	height: 140px;
	box-sizing: border-box;
	padding: 10px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.protocol{
	width: 720px;
	height:20px;
	line-height: 20px;
	box-sizing: border-box;
	padding: 0 120px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	margin-top: 5px;
}

.protocol input{
	display: inline-block;
	width: 14px;
	height: 14px;
	margin-top: 3px;
	float: left;
}

.protocol span:hover{
	text-decoration: underline;
	color: #e02341;
	cursor: pointer;
}

.offerInfo{
	width: 720px;
	height: 120px;
	border-top: 1px solid #e8e8e8;
	overflow-y: auto; 
}

.offerInfo ul{
	width: 640px;
	margin-left: 20px;
}

.offer{
	width: 640px;
	height: 40px;
	line-height: 40px;
	text-align: center;
}

.select{
	float: left;
	display: inline-block;
	width:40px;
	height: 40px;
	text-align: center;
	line-height: 40px;
}

.select input{
	display: inline-block;
	width:20px;
	height: 20px;
	margin-top: 10px;
}

.Business{
	float: left;
	display: inline-block;
	width: 360px;
	height: 40px;
	text-align: center;
	line-height: 40px;
}

.quote,.carriage{
	float: left;
	display: inline-block;
	width: 120px;
	height: 40px;
	text-align: center;
	line-height: 40px;
}

.btn{
	width: 720px;
	height: 40px;
	box-sizing: border-box;
	padding: 0 20px;
}

.btn span{
	float: right;
}

.modify,.confirm{
	display: inline-block;
	width: 100px;
	height: 32px;
	line-height: 32px;
	text-align: center;
	border: none;
	background: none;
	background: #e02341;
	color: #ffffff;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	border-radius: 5px;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-ms-border-radius: 5px;
	-o-border-radius: 5px;
}

.agreementMode{
	display: none;
	position: fixed;
	width: 100%;
	height: 100%;
	z-index: 999;
	background: rgba(0,0,0,0.2);
	top: 0px;
	left: 0px;
}

.agreement{
	width: 722px;
	height: 562px;
	margin: 0 auto;
	border: 1px solid #e8e8e8;
	background: #ffffff;
}

.agreementTtl{
	width: 722px;
	height: 32px;
	box-sizing: border-box;
	padding: 0 20px;
	line-height: 32px;
	border-bottom: 1px solid #e8e8e8;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
	position: relative;
}

.agreementTtl img{
	width: 18px;
	height: 18px;
	position: absolute;
	top: 7px;
	right: 7px;
}

.agreementContent{
	width: 722px;
	height: 530px;
	box-sizing: border-box;
	padding: 20px;
	line-height: 18px;
	color: #4a4a4a;
	font-size: 16px;
	font-family: "Microsoft YaHei";
}