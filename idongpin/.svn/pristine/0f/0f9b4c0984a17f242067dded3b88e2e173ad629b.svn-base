.miscellaneous{
	width: 948px;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft Yahei";
}

.miscellaneousTopic{
	width: 122px;
	height: 34px;
	line-height: 34px;
	box-sizing: border-box;
	padding-left: 20px;
	float: left;
}

.miscellaneousContent{
	width: 826px;
	float: left;
}

.miscellaneoue{
	height: 14px;
	line-height: 14px;
	box-sizing: border-box;
	padding: 0 10px;
	float: left;
	margin: 10px 0px;
	border-right: 1px solid #cccccc;
}

.miscellaneoue a{
	color: #a1a1a1;
}

.miscellaneoue a:hover{
	color: #ff6c00;
}

.more:hover,.selectMore:hover{
	cursor: pointer;
}


