.carte {
	height: 0px;
}

.secondCarte,.firstCarte{
	display: none;
}

#showMenu{
	transform : rotate(180deg);
	-webkit-transform : rotate(180deg);
	-moz-transform : rotate(180deg);
	-ms-transform : rotate(180deg);
	-o-transform : rotate(180deg);
}

.ad{
	display: none;
}

.content{
	width: 980px;
	margin: 0 auto;
	box-shadow: 0 0 20px #f2f6f7;
	-webkit-box-shadow: 0 0 40px #f2f6f7;
	-moz-box-shadow: 0 0 40px #f2f6f7;
	-ms-box-shadow: 0 0 40px #f2f6f7;
	-o-box-shadow: 0 0 40px #f2f6f7;
	box-sizing: border-box;
	padding: 14px 15px;
}

.filter{
	width: 950px;
}

.filterNav{
	width: 950px;
	height: 42px;
	font-size: 12px;
	font-family: "Microsoft Yahei";
	line-height: 42px;
	box-sizing: border-box;
	padding: 0 20px;
}

.startNav{
	color: #ff6c00;
}

.filterNav a{
	padding: 0 5px;
}

.filterNav a:hover{
	color: #ff6c00;
}

.filterCondition{
	width: 948px;
	border: 1px solid #cccccc;
}

.confirm a{
	display: block;
}

.showCondition{
	width: 948px;
	border-bottom: 1px solid #cccccc;
	zoom: 1;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft Yahei";
}

.showCondition:after{
	clear: both;
	content: "";
	display: block;
}

.conditionTopic{
	width: 122px;
	height: 33px;
	line-height: 33px;
	box-sizing: border-box;
	padding: 0 20px;
	float: left;
}

.conditionContent{
	width: 826px;
	float: left;
}

.selectedCondition{
	height: 22px;
	border: 1px solid #cccccc;
	float: left;
	margin: 5px 10px;
}

.conditionKind{
	float: left;
	height: 22px;
	line-height: 24px;
	padding-left: 18px;
	padding-right: 8px;
}

.condition{
	float: left;
	height: 22px;
	line-height: 22px;
}

.close{
	float: left;
	width: 32px;
	height: 22px;
	text-align: center;
	margin-top: 5px;
}

.brands{
	width: 948px;
	border-bottom: 1px solid #cccccc;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft Yahei";
}

.brandTopic{
	width: 102px;
	height: 33px;
	line-height: 33px;
	box-sizing: border-box;
	padding: 0 20px;
	float: left;
}

.brandContent{
	width: 704px;
	float: left;
	height: 33px;
	overflow: hidden;
}

.brands .otherHandle{
	width: 142px;
	float: left;
}

.brand{
	width: 114px;
	height: 24px;
	line-height: 24px;
	box-sizing: border-box;
	padding: 0 10px;
	float: left;
	margin: 5px 0px;
}

.brand:hover{
	cursor: pointer;
	color: #ff6c00;
}

.brand img{
	visibility: hidden;
	width: 14px;
	height: 14px;
	margin-top: 5px;
	float: left;
	margin-right: 8px;
}

.brands .otherHandle div{
	width: 54px;
	height: 22px;
	border: 1px solid #cccccc;
	float: left;
	margin-left: 10px;
	margin-top: 5px;
	line-height: 22px;
	text-align: center;
}

.brands .otherHandle .more img,.brands .otherHandle .selectMore img{
	width: 7px;
	height: 8px;
	margin-left: 2px;
}

.brands .otherHandle .cancel img{
	width: 4px;
	height: 2px;
	margin-left: 2px;
	margin-bottom: 4px;
}

