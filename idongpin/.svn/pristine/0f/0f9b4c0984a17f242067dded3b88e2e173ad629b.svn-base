.goodsMode{
	width: 950px;
	margin: 0 auto;
	overflow: auto;
	zoom: 1;
	box-sizing: border-box;
	border-top: 1px solid #e8e8e8;
	border-left: 1px solid #e8e8e8;
}

.modeActive{
	display: block;
}

.goods li{
	position: relative;
	width: 189.8px;
	height: 333.2px;
	float: left;
	box-sizing: border-box;
	padding: 20px;
	cursor: pointer;
	border-bottom: 1px solid #e8e8e8;
	border-right: 1px solid #e8e8e8;
}

.hotGoodsSign{
	position: absolute;
	top: 5px;
	right: 5px;
}
.hotGoodsSign img{
	width: 38px;
	height: 24px;
	right: 0;
}
.goodsImg{
	width: 149.8px;
	height: 149.8px;
	overflow: hidden;
}

.goodsImg img{
	width: 100%;
	height: 100%;
}

.goodsIntroduce{
	width: 149.8px;
	height: 33px;
	margin-top: 20px;
}

.goodsName{
	width: 149.8px;
	height: 15px;
	color: #4a4a4a;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.goodsKind{
	width: 149.8px;
	height: 13px;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft YaHei";
	margin-top: 5px;
}

.goodsInfo{
	width: 149.8px;
	height: 39px;
	margin-top: 20px;
}

.priceNow{
	width: 149.8px;
	height: 21px;
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.now{
	color: #ff6c00;
	font-size: 22px;
	font-family: "Microsoft YaHei";
}

.cost{
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
}

.regularPrice{
	width: 148px;
	height: 12px;
	margin-top: 6px;
	color: #a1a1a1;
	font-size: 14px;
	font-family: "Microsoft YaHei";
	text-decoration: line-through;
}

.proUnitPrice{
	float: right;
}

.belong{
	width: 148px;
	height: 12px;
	margin-top: 10px;
}

.belong img{
	float: left;
	width: 13px;
	height: 13px;
	margin-top: -1px;
}

.company{
	float: left;
	margin-left: 10px;
	color: #a1a1a1;
	font-size: 12px;
