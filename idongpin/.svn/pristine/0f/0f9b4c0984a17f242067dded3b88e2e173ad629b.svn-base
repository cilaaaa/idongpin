}

.orderOptions{
	height: 24px;
	margin: 4px 20px;
}

.option{
	float: left;
	border: 1px solid #cccccc;
	padding:  3px 16px;
	color: #a1a1a1;
	font-size: 12px;
	font-family: "Microsoft Yahei";
	background-color: #ffffff;
	position: relative;
}

.option:hover{
	cursor: pointer;
}

.optionActive{
	background-color: #c02341;
	color: #ffffff;
	border: 1px solid #c02341;
}

.option img{
	width: 6px;
	height: 12px;
	position: absolute;
	top: 5px;
}

